{
    "$schema": "http://json.schemastore.org/template",
    "author": "Sayed I. Hashimi",
    "classifications": [ "Web" ],
    "name": "sayedweb",
    "shortName": "sayedweb",
    "defaultName": "MyWeb",
    "identity": "Sayedha.Web.CSharp",
    "groupIdentity":"Sayedha.Web",    
    "tags": {
      "language": "C#",
      "type":"project"
    },
    "sourceName": "MyWebApp",
    "preferNameDirectory":true,
    "symbols":{
      "Framework": {
        "type": "parameter",
        "description": "The target framework for the project.",
        "datatype": "choice",
        "choices": [
          {
            "choice": "netcoreapp3.1",
            "description": "Target netcoreapp3.1"
          }
        ],
        "replaces": "netcoreapp3.1",
        "defaultValue": "netcoreapp3.1"
      },
      "authorName": {
        "type": "parameter",
        "defaultValue": "(insert author name)",
        "replaces":"AuthorName",
        "datatype":"text",
        "description": "Author name"
      },
      "siteTitle": {
        "type": "parameter",
        "description": "Title that will be shown in the browser tab",
        "defaultValue": "(insert title)",
        "replaces":"SiteTitle"
      }
    }
  }